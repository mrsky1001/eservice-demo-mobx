{
  "name": "eservice-demo-mobx",
  "version": "1.0.0",
  "description": "New version on ReactJs",
  "scripts": {
    "update": "(cd src/; . ./get_core.sh)",
    "init": "(cd src/core/; . ./bash/init.sh) ",
    "commit": "(cd src/core/; . ./bash/commit.sh) ",
    "push": "(cd src/core/; . ./bash/push.sh) ",
    "dev": "ts-node src/core/proxy.ts",
    "dev2": "node src/core/proxy_devel2.js",
    "dev-fix": "npm run pretty; npm run fix; node src/core/proxy.js",
    "dev2-fix": "npm run valid; npm run fix; node src/core/proxy_devel2.js",
    "build": "npm run pretty && npm run fix && yaspeller . && parcel build src/core/index.html --public-url ./",
    "pretty": "prettier --check \"src/**/*.{ts, tsx, scss}\"",
    "fix": "tslint --project tsconfig.json --fix --force",
    "kill-back": "kill -9 `lsof -t -i tcp:4567`",
    "kill-front": "kill -9 `lsof -t -i tcp:1234`"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{ts, tsx, js}": [
      "prettier --write --config .prettierr.yaml",
      "tslint --project tsconfig.json --fix --force",
      "sh lint.sh partner-account",
      "git add"
    ],
    "*.{html,scss,css}": [
      "prettier --write --config .prettierr.yaml",
      "git add"
    ]
  },
  "author": "Mr.Sky1001",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/omsueservice/eservice-demo-mobx.git"
  },
  "bugs": {
    "url": "https://github.com/omsueservice/eservice-demo-mobx/issues"
  },
  "homepage": "https://github.com/omsueservice/eservice-demo-mobx#readme",
  "dependencies": {
    "ajax": "0.0.4",
    "bootstrap": "^4.5.3",
    "date-fns": "^2.16.1",
    "font-awesome": "^4.7.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "node-sass": "^4.14.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.12.21",
    "react-mobx": "0.0.4",
    "react-overlay-loader": "0.0.3",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^10.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@types/node": "^12.7.12",
    "@types/parcel-env": "0.0.0",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "codelyzer": "^6.0.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.3",
    "parcel-proxy-server": "^1.1.0",
    "prettier": "^2.2.0",
    "react-toastify": "^6.1.0",
    "sass": "^1.26.10",
    "sys": "0.0.1",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "tslint": "^6.1.3",
    "yandex-speller": "^4.1.0"
  }
}
